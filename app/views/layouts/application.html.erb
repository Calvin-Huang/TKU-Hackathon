<!DOCTYPE html>
<html>
<head>
  <title>Hackathon2014</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag 'unify', "data-turbolinks-track" => true %>
  <%= javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=true' %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="localscroll">
  <div id="mainHeader" class="navbar-fixed-top">
    <div class="top-v1">
      <%= render 'layouts/header_top' %>
    </div>
    <header class="header" style="display:block;background-color:#fff;">
      <%= render 'layouts/header_navigation' %>
    </header>
  </div>

  <div class="one-page" style="margin-top:110px;">
    <%= yield %>
  </div>
</div>

<div id="topcontrol">
  <%= image_tag 'unify/up.png', style: 'width:51px;height:42px;', width: '51', height: '42' %>
</div>

<div class="footer">
  <%= render 'layouts/footer' %>
</div>

<%= javascript_include_tag 'application', data: { 'turbolinks-track' => true } %>
</body>
</html>
